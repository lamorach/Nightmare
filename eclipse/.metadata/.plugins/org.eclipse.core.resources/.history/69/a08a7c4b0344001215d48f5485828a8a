	{
		super(par1);
		setTextureFile("nightmareRalmGFX/Items.png");
		setItemName("SoulshadeOre");
		setCreativeTab(CreativeTabs.tabMaterials);
	}
	
	@SideOnly(Side.CLIENT)
	public int getItemIconFromDamge(int i)
	{
		return 0;
	}

}
